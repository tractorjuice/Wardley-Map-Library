{
  "name": "wardley-map-library",
  "version": "1.0.0",
  "description": "A comprehensive library of Wardley Mapping resources, books, and strategic analysis reports",
  "main": "api/index.js",
  "scripts": {
    "build": "echo \"Build completed\"",
    "vercel-build": "echo \"Vercel build completed\"",
    "generate-manifest": "node scripts/generate-manifest.js",
    "generate-manifest-with-toc": "node scripts/generate-manifest.js --include-toc",
    "generate-toc": "node scripts/table-of-contents-generator.js --anchors",
    "generate-toc-dry": "node scripts/table-of-contents-generator.js --dry-run",
    "update-books-with-figures": "node scripts/update-books-with-figures.js",
    "add-wardley-appendix": "node scripts/add-wardley-appendix.js",
    "build-all": "npm run generate-toc && npm run generate-manifest"
  },
  "keywords": [
    "wardley-mapping",
    "strategic-analysis",
    "business-strategy",
    "library",
    "vercel",
    "serverless"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "marked": "^4.3.0",
    "xml2js": "^0.6.2"
  }
}
